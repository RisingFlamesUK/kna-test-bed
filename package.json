{
  "name": "kickstart-node-app-testbed",
  "version": "0.4.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier . -c",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:e2e": "vitest run",
    "test:e2e:watch": "vitest --watch",
    "test:pre": "node scripts/test-pre.mjs",
    "test:pre:report": "cross-env ALLOW_TEST_FAILURES=1 node scripts/test-pre.mjs",
    "prepare": "husky",
    "ci:validate:prompt-map": "ajv validate --spec=draft2020 -s test/e2e/schema/config/prompt-map.schema.json -d test/e2e/schema/fixtures/prompt-map.json --strict=false",
    "ci:validate:prompt-maps": "ajv validate --spec=draft2020 -s test/e2e/schema/config/prompt-map.schema.json -d \"test/e2e/**/prompt-map.json\" --strict=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.6.0",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vitest": "^0.5.4",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "pg": "^8.16.3",
    "picomatch": "^4.0.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@typescript-eslint/utils": "^8",
    "fast-json-patch": "^3.1.1"
  },
  "dependencies": {
    "-": "^0.0.1"
  }
}
